{
    "compilerOptions": {
     "jsx": "react",
     "module": "commonjs",
     "moduleResolution": "node",
     "noImplicitAny": false,
     "outDir": "./build/",
     "preserveConstEnums": true,
     "downlevelIteration": true,
     "removeComments": true,
     "target": "ES5",
     "esModuleInterop": true,
     "lib": ["es5", "es2015", "dom"],
     "declaration": false,
     "baseUrl": ".",
     "paths": {
         "@src": ["./src"],
     }
    }
}