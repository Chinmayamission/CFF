    "test": "jest",
    "tdd": "jest --watch",
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --progress --config webpack.beta.js",
    "build-sheets": "serverless package --stage beta",
    "deploy": "CFF_DEPLOY_TO=beta pipenv run python deploy.py",
    "deploy-sheets": "serverless deploy --stage beta --force",
    "build-prod": "webpack --progress --config webpack.prod.js",
    "build-prod-sheets": "serverless package --stage prod",
    "deploy-prod": "CFF_DEPLOY_TO=prod pipenv run python deploy.py",
    "deploy-prod-sheets": "serverless deploy --stage prod --force",
    "docs": "mkdocs serve",
    "build-docs": "mkdocs build --clean",
    "deploy-docs": "npm run build-docs && aws s3 sync ./site s3://docs.chinmayamission.com/cff --delete --region us-east-1",
    "start-gs": "googleKey=CFF_GOOGLE_SHEETS_KEY_BETA mapsApiKey=CFF_MAPS_API_KEY_BETA mongoCollectionName=cff_beta mongoConnStr=CFF_ATLAS_CONN_STR_WRITE_BETA ts-node scripts/backend/local-run.ts",
